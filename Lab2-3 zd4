#include <stdio.h>

int main() {
    int age;
    
    scanf("%d", &age);
    
    if ((0 <= age) && (age <= 6))
        printf("дошкольник");
        
    else if ((7 <= age) && (age <= 18))
        printf("школьник");
        
    else if ((19 <= age) && (age <= 59))
        printf("рабочий");
        
    else if (age >= 60)
        printf("пенсионер");
        
    return 0;
  
}
